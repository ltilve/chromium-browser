    ../../native_client/src/trusted/service_runtime/linux/nacl_bootstrap.x $
    gen/nacl_bootstrap/libnacl_bootstrap_lib.a $
    ../../native_client/src/trusted/service_runtime/linux/ld_bfd.py || $
